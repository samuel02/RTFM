#! /bin/sh

in=$1

echo cOOre-compiler ====================================================
../../RTFM-cOOre-compiler/src/Main.native -i $1 -o ${in%.*}.core
echo core-compiler =====================================================
../../RTFM-core-compiler/src/Main.native -i ${in%.*}.core -o ../Application/autogen.c
echo gcc ===============================================================
gcc ../RTFM-PT/RTFM-PT.c -o ${in%.*} -lpthread


