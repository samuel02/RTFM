// The syncronous Func emits an asynchronous task
Task task(int i, float j) {
    #> printf("task %d %f\n", i, j);<#
}

Func void f() {
	async after 3000000 prio 1 task(2, 5);
}

Reset {
    #> printf("Hello\n");<#
    async after 3000000 prio 1 task(7, 8);
    sync f();
    async after 2000000 prio 2 task(2, 2);
}
